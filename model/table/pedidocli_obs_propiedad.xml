<tabla>
    <columna>
        <nombre>name</nombre>
        <tipo>character varying(50)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idpedido</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>text</nombre>
        <tipo>text</tipo>
    </columna>
    <restriccion>
        <nombre>pedidocli_obs_propiedades_pkey</nombre>
        <consulta>PRIMARY KEY (name,idpedido)</consulta>
    </restriccion>
    <restriccion>
        <nombre>ca_pedidocli_obs_propiedades_pedidos</nombre>
        <consulta>FOREIGN KEY (idpedido)
            REFERENCES pedidos (idpedido)
            ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>